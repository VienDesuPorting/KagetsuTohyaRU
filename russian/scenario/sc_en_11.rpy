# game/scenario/sc_en.rpy:34869
translate russian s529_b08d5d7e:

    # nvl clear
    nvl clear
    # "\"---Shut up!{w} Don't you dare come in, Tohno!\""
    "\"---Заткнись!{w} Не смей входить, Тоно!\""
# game/scenario/sc_en.rpy:34871
translate russian s529_1c1d95ea_1:

    # " {nw}"
    " {nw}"

# game/scenario/sc_en.rpy:34872
